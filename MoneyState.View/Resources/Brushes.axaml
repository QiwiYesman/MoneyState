<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="DarkButtonBrush">Black</SolidColorBrush>
    <SolidColorBrush x:Key="LightButtonBrush">Cyan</SolidColorBrush>
    <SolidColorBrush x:Key="DarkerLightButtonBrush">DarkCyan</SolidColorBrush>
    
    <SolidColorBrush x:Key="DarkBackgroundBrush">#131517</SolidColorBrush>
    
    <SolidColorBrush x:Key="SelectedLightBrush">Lime</SolidColorBrush>
    <SolidColorBrush x:Key="SelectedDarkBrush">DarkBlue</SolidColorBrush>
    <SolidColorBrush x:Key="SelectedTextLightBrush">LightCyan</SolidColorBrush>
    <SolidColorBrush x:Key="SelectedTextDarkBrush">Black</SolidColorBrush>
    
